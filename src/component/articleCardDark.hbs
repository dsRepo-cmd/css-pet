{{#each data}}
<div class='card'>
<div class='card_img_box'>
    <img class='card_img' src='{{photo}}' alt='' />
    <div id="{{id}}" {{#unless isNew}} style="display:none;" {{/unless}} class='card_badge'>NEW</div>
</div>
<h3 class='card_title'>{{name}}</h3>
<p class='card_text'>{{description}}</p>
<button class='card_button'>Read All</button>
</div>
{{/each}}